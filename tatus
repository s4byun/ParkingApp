[32m+[m[32mpackage com.example.danby.myapplication;[m
[32m+[m
[32m+[m[32mimport android.content.Intent;[m
[32m+[m[32mimport android.os.Bundle;[m
[32m+[m[32mimport android.support.v4.app.FragmentActivity;[m
[32m+[m[32mimport android.util.Log;[m
[32m+[m[32mimport android.view.View;[m
[32m+[m
[32m+[m[32mimport com.facebook.CallbackManager;[m
[32m+[m[32mimport com.facebook.FacebookCallback;[m
[32m+[m[32mimport com.facebook.FacebookException;[m
[32m+[m[32mimport com.facebook.login.LoginResult;[m
[32m+[m[32mimport com.facebook.login.widget.LoginButton;[m
[32m+[m
[32m+[m
[32m+[m[32mpublic class LoginActivity extends FragmentActivity {[m
[32m+[m[32m    private LoginButton loginButton;[m
[32m+[m[32m    private CallbackManager callbackManager;[m
[32m+[m
[32m+[m[32m    @Override[m
[32m+[m[32m    protected void onCreate(Bundle savedInstanceState) {[m
[32m+[m[32m        super.onCreate(savedInstanceState);[m
[32m+[m[32m        callbackManager = CallbackManager.Factory.create();[m
[32m+[m[32m        setContentView(R.layout.activity_login);[m
[32m+[m[32m        loginButton = (LoginButton) findViewById(R.id.login_button);[m
[32m+[m[32m        loginButton.setReadPermissions("user_friends");[m
[32m+[m[32m        loginButton.registerCallback(callbackManager, new FacebookCallback<LoginResult>() {[m
[32m+[m[32m            @Override[m
[32m+[m[32m            public void onSuccess(LoginResult loginResult) {[m
[32m+[m[32m                Log.i("fb_login", "Login success: " + loginResult.toString());[m
[32m+[m[32m                loginButton.setVisibility(View.GONE);[m
[32m+[m[32m                setResult(AppConfig.FB_LOGIN_RETURN_CODE_SUCCESS);[m
[32m+[m[32m                startActivity(new Intent(LoginActivity.this, MapsActivity.class));[m
[32m+[m[32m                finish();[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            @Override[m
[32m+[m[32m            public void onCancel() {[m
[32m+[m[32m                Log.i("fb_login", "Login cancelled");[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            @Override[m
[32m+[m[32m            public void onError(FacebookException e) {[m
[32m+[m[32m                Log.e("fb_login", e.toString());[m
[32m+[m[32m                setResult(AppConfig.FB_LOGIN_RETURN_CODE_FAIL);[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    @Override[m
[32m+[m[32m    protected void onActivityResult(int requestCode, int resultCode, Intent data) {[m
[32m+[m[32m        super.onActivityResult(requestCode, resultCode, data);[m
[32m+[m[32m        callbackManager.onActivityResult(requestCode, resultCode, data);[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
