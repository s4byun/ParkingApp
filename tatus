[32m+[m[32m    private void ensureFBLogin() {[m
[32m+[m[32m        accessTokenTracker = new AccessTokenTracker() {[m
[32m+[m[32m            @Override[m
[32m+[m[32m            protected void onCurrentAccessTokenChanged(AccessToken oldAccessToken, AccessToken currentAccessToken) {[m
[32m+[m[32m                accessToken = currentAccessToken;[m
[32m+[m[32m            }[m
[32m+[m[32m        };[m
[32m+[m[32m        accessToken = AccessToken.getCurrentAccessToken();[m
[32m+[m[32m        if (accessToken == null) {[m
[32m+[m[32m            Intent intent = new Intent(this, LoginActivity.class);[m
[32m+[m[32m            startActivityForResult(intent, LOGIN_ACTIVITY_REQUEST_CODE);[m
[32m+[m[32m        } else {[m
[32m+[m[32m            logoutButton = (Button) findViewById(R.id.logout_button);[m
[32m+[m[32m            logoutButton.setOnClickListener(new View.OnClickListener() {[m
[32m+[m[32m                @Override[m
[32m+[m[32m                public void onClick(View v) {[m
[32m+[m[32m                    accessToken = AccessToken.getCurrentAccessToken();[m
[32m+[m[32m                    if (accessToken != null) {[m
[32m+[m[32m                        LoginManager.getInstance().logOut();[m
[32m+[m[32m                    }[m
[32m+[m[32m                    Intent intent = new Intent(MapsActivity.this, LoginActivity.class);[m
[32m+[m[32m                    startActivityForResult(intent, LOGIN_ACTIVITY_REQUEST_CODE);[m
[32m+[m[32m                }[m
[32m+[m[32m            });[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m
