[32m+[m
[32m+[m[32m    @Override[m
[32m+[m[32m    protected void onActivityResult(int requestCode, int resultCode, Intent data) {[m
[32m+[m[32m        super.onActivityResult(requestCode, resultCode, data);[m
[32m+[m[32m        if (requestCode == LOGIN_ACTIVITY_REQUEST_CODE) {[m
[32m+[m[32m            if (resultCode == AppConfig.FB_LOGIN_RETURN_CODE_SUCCESS) {[m
[32m+[m[32m                Log.i("fb_login", "MapsActivity received FB_LOGIN_RETURN_CODE_SUCCESS");[m
[32m+[m[32m            } else if (resultCode == AppConfig.FB_LOGIN_RETURN_CODE_FAIL) {[m
[32m+[m[32m                Log.e("fb_login", "MapsActivity received FB_LOGIN_RETURN_CODE_FAIL");[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    @Override[m
[32m+[m[32m    public void onDestroy() {[m
[32m+[m[32m        super.onDestroy();[m
[32m+[m[32m        accessTokenTracker.stopTracking();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Manipulates the map once available.[m
[32m+[m[32m     * This callback is triggered when the map is ready to be used.[m
[32m+[m[32m     * This is where we can add markers or lines, add listeners or move the camera. In this case,[m
[32m+[m[32m     * we just add a marker near Sydney, Australia.[m
[32m+[m[32m     * If Google Play services is not installed on the device, the user will be prompted to install[m
[32m+[m[32m     * it inside the SupportMapFragment. This method will only be triggered once the user has[m
[32m+[m[32m     * installed Google Play services and returned to the app.[m
[32m+[m[32m     */[m
[32m+[m[32m    @Override[m
[32m+[m[32m    public void onMapReady(GoogleMap googleMap) {[m
[32m+[m[32m        mMap = googleMap;[m
[32m+[m
[32m+[m[32m        //Initialize Google Play Services[m
[32m+[m[32m        if (android.os.Build.VERSION.SDK_INT >= Build.VERSION_CODES.M) {[m
[32m+[m[32m            if (ContextCompat.checkSelfPermission(this,[m
[32m+[m[32m                    Manifest.permission.ACCESS_FINE_LOCATION)[m
[32m+[m[32m                    == PackageManager.PERMISSION_GRANTED) {[m
[32m+[m[32m                buildGoogleApiClient();[m
[32m+[m[32m                mMap.setMyLocationEnabled(true);[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m        else {[m
[32m+[m[32m            buildGoogleApiClient();[m
[32m+[m[32m            mMap.setMyLocationEnabled(true);[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    protected synchronized void buildGoogleApiClient() {[m
[32m+[m[32m        mGoogleApiClient = new GoogleApiClient.Builder(this)[m
[32m+[m[32m                .addConnectionCallbacks(this)[m
[32m+[m[32m                .addOnConnectionFailedListener(this)[m
[32m+[m[32m                .addApi(LocationServices.API)[m
[32m+[m[32m                .build();[m
[32m+[m[32m        mGoogleApiClient.connect();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    @Override[m
[32m+[m[32m    public void onConnected(@Nullable Bundle bundle) {[m
[32m+[m[32m        mLocationRequest = new LocationRequest();[m
[32m+[m[32m        mLocationRequest.setInterval(1000);[m
[32m+[m[32m        mLocationRequest.setFastestInterval(1000);[m
[32m+[m[32m        mLocationRequest.setPriority(LocationRequest.PRIORITY_BALANCED_POWER_ACCURACY);[m
[32m+[m[32m        if (ContextCompat.checkSelfPermission(this,[m
[32m+[m[32m                Manifest.permission.ACCESS_FINE_LOCATION)[m
[32m+[m[32m                == PackageManager.PERMISSION_GRANTED) {[m
[32m+[m[32m            LocationServices.FusedLocationApi.requestLocationUpdates(mGoogleApiClient, mLocationRequest, this);[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public boolean checkLocationPermission(){[m
[32m+[m[32m        if (ContextCompat.checkSelfPermission(this,[m
[32m+[m[32m                Manifest.permission.ACCESS_FINE_LOCATION)[m
[32m+[m[32m                != PackageManager.PERMISSION_GRANTED) {[m
[32m+[m
[32m+[m[32m            // Asking user if explanation is needed[m
[32m+[m[32m            if (ActivityCompat.shouldShowRequestPermissionRationale(this,[m
[32m+[m[32m                    Manifest.permission.ACCESS_FINE_LOCATION)) {[m
[32m+[m
[32m+[m[32m                // Show an explanation to the user *asynchronously* -- don't block[m
[32m+[m[32m                // this thread waiting for the user's response! After the user[m
[32m+[m[32m                // sees the explanation, try again to request the permission.[m
[32m+[m
[32m+[m[32m                //Prompt the user once explanation has been shown[m
[32m+[m[32m                ActivityCompat.requestPermissions(this,[m
[32m+[m[32m                        new String[]{Manifest.permission.ACCESS_FINE_LOCATION},[m
[32m+[m[32m                        LOCATION_REQUEST_CODE);[m
[32m+[m[32m            } else {[m
[32m+[m[32m                // No explanation needed, we can request the permission.[m
[32m+[m[32m                ActivityCompat.requestPermissions(this,[m
[32m+[m[32m                        new String[]{Manifest.permission.ACCESS_FINE_LOCATION},[m
[32m+[m[32m                        LOCATION_REQUEST_CODE);[m
[32m+[m[32m            }[m
[32m+[m[32m            return false;[m
[32m+[m[32m        } else {[m
[32m+[m[32m            return true;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    @Override[m
[32m+[m[32m    public void onRequestPermissionsResult(int requestCode, String permissions[], int[] grantResults) {[m
[32m+[m[32m        switch (requestCode) {[m
[32m+[m[32m            case LOCATION_REQUEST_CODE: {[m
[32m+[m[32m                // If request is cancelled, the result arrays are empty.[m
[32m+[m[32m                if (grantResults.length > 0[m
[32m+[m[32m                        && grantResults[0] == PackageManager.PERMISSION_GRANTED) {[m
[32m+[m
[32m+[m[32m                    // Permission was granted.[m
[32m+[m[32m                    if (ContextCompat.checkSelfPermission(this,[m
[32m+[m[32m                            Manifest.permission.ACCESS_FINE_LOCATION)[m
[32m+[m[32m                            == PackageManager.PERMISSION_GRANTED) {[m
[32m+[m
[32m+[m[32m                        if (mGoogleApiClient == null) {[m
[32m+[m[32m                            buildGoogleApiClient();[m
[32m+[m[32m                        }[m
[32m+[m[32m                        mMap.setMyLocationEnabled(true);[m
[32m+[m[32m                    }[m
[32m+[m
[32m+[m[32m                } else {[m
[32m+[m[32m                    // Permission denied, Disable the functionality that depends on this permission.[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            // other 'case' lines to check for other permissions this app might request.[m
[32m+[m[32m            //You can add here other case statements according to your requirement.[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    @Override[m
[32m+[m[32m    public void onConnectionSuspended(int i) {[m
[32m+[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    @Override[m
[32m+[m[32m    public void onConnectionFailed(@NonNull ConnectionResult connectionResult) {[m
[32m+[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    @Override[m
[32m+[m[32m    public void onLocationChanged(Location location) {[m
[32m+[m[32m        mLastLocation = location;[m
[32m+[m[32m        if (mCurrLocationMarker != null) {[m
[32m+[m[32m            mCurrLocationMarker.remove();[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        //Place current location marker[m
[32m+[m[32m        LatLng latLng = new LatLng(location.getLatitude(), location.getLongitude());[m
[32m+[m[32m        MarkerOptions markerOptions = new MarkerOptions();[m
[32m+[m[32m        markerOptions.flat(true);[m
[32m+[m[32m        markerOptions.position(latLng);[m
[32m+[m[32m        markerOptions.title("Current Position");[m
[32m+[m[32m        markerOptions.icon(BitmapDescriptorFactory.defaultMarker(BitmapDescriptorFactory.HUE_MAGENTA));[m
[32m+[m[32m        mCurrLocationMarker = mMap.addMarker(markerOptions);[m
[32m+[m
[32m+[m[32m        //move map camera[m
[32m+[m[32m        mMap.moveCamera(CameraUpdateFactory.newLatLngZoom(latLng, 11));[m
[32m+[m
[32m+[m[32m        //stop location updates[m
[32m+[m[32m        if (mGoogleApiClient != null) {[m
[32m+[m[32m            LocationServices.FusedLocationApi.removeLocationUpdates(mGoogleApiClient, this);[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
